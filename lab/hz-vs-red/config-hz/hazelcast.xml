<hazelcast xsi:schemaLocation="http://www.hazelcast.com/schema/config
                               http://www.hazelcast.com/schema/config/hazelcast-config-3.6.xsd"
           xmlns="http://www.hazelcast.com/schema/config"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <group>
        <name>group</name>
    </group>

    <network>
        <port port-count="200" auto-increment="true">5701</port>
        <join>
            <multicast enabled="false"/>
            <tcp-ip enabled="true">
            </tcp-ip>
        </join>
   
        <ssl enabled="false">
            <factory-class-name>
                com.hazelcast.nio.ssl.BasicSSLContextFactory
            </factory-class-name>
            <properties>
                <property name="javax.net.ssl.keyStore">hazelcast.keystore</property>
                <property name="javax.net.ssl.keyStorePassword">123456</property>
                <property name="javax.net.ssl.trustStore">hazelcast.truststore</property>
                <property name="javax.net.ssl.trustStorePassword">123456</property>
                <property name="keyManagerAlgorithm">SunX509</property>
                <property name="trustManagerAlgorithm">SunX509</property>
                <property name="protocol">TLS</property>
            </properties>
        </ssl>

    </network>

    <native-memory allocator-type="POOLED" enabled="true">
        <size unit="GIGABYTES" value="50"/>
        <metadata-space-percentage>15</metadata-space-percentage>
    </native-memory>



    <map name="offheap*">
        <in-memory-format>NATIVE</in-memory-format>
        <eviction-policy>LRU</eviction-policy>
        <eviction-percentage>5</eviction-percentage>        
        <max-size policy="FREE_NATIVE_MEMORY_PERCENTAGE">5</max-size>
        <min-eviction-check-millis>0</min-eviction-check-millis>
        <statistics-enabled>false</statistics-enabled>
    </map>

    <map name="AsyncBakUpOffheap*">
        <in-memory-format>NATIVE</in-memory-format>
        <eviction-policy>LRU</eviction-policy>
        <eviction-percentage>5</eviction-percentage>
        <max-size policy="FREE_NATIVE_MEMORY_PERCENTAGE">5</max-size>
        <min-eviction-check-millis>0</min-eviction-check-millis>
        <statistics-enabled>false</statistics-enabled>
        <backup-count>0</backup-count>
        <async-backup-count>1</async-backup-count>
    </map>

    <map name="syncBak-1_onHeap*">
        <in-memory-format>BINARY</in-memory-format>
        <backup-count>1</backup-count>
    </map>


    <map name="AsyncBak-1_onHeap*">
        <in-memory-format>BINARY</in-memory-format>
	<backup-count>0</backup-count>
        <async-backup-count>1</async-backup-count>
    </map>


    <map name="hz-0bakUp*">
        <in-memory-format>BINARY</in-memory-format>
        <backup-count>0</backup-count>
        <async-backup-count>0</async-backup-count>
    </map>


    <cache name="hzDefaultOffheap*">
        <backup-count>1</backup-count>
        <in-memory-format>NATIVE</in-memory-format>
        <eviction size="99" max-size-policy="USED_NATIVE_MEMORY_PERCENTAGE" eviction-policy="LRU" />
        <statistics-enabled>true</statistics-enabled>
    </cache>

</hazelcast>
