<!DOCTYPE cache-config SYSTEM "cache-config.dtd">
<cache-config>
    <caching-scheme-mapping>

        <cache-mapping>
            <cache-name>cacheBak0*</cache-name>
            <scheme-name>cacheBak0</scheme-name>
        </cache-mapping>

        <cache-mapping>
            <cache-name>cacheBak1*</cache-name>
            <scheme-name>cacheBak1</scheme-name>
        </cache-mapping>

        <cache-mapping>
            <cache-name>cacheAsync1*</cache-name>
            <scheme-name>cacheAsync1</scheme-name>
        </cache-mapping>

    </caching-scheme-mapping>

    <caching-schemes>
        <distributed-scheme>
            <scheme-name>cacheBak0</scheme-name>
            <backing-map-scheme>
                <local-scheme>
                </local-scheme>
            </backing-map-scheme>
            <backup-count>0</backup-count>
            <autostart>true</autostart>
        </distributed-scheme>

        <distributed-scheme>
            <scheme-name>cacheBak1</scheme-name>
            <backing-map-scheme>
                <local-scheme>
                </local-scheme>
            </backing-map-scheme>
            <backup-count>1</backup-count>
            <autostart>true</autostart>
        </distributed-scheme>

        <distributed-scheme>
            <scheme-name>cacheAsync1</scheme-name>
            <backing-map-scheme>
                <local-scheme>
                </local-scheme>
            </backing-map-scheme>
            <backup-count>1</backup-count>
            <async-backup>true</async-backup>
            <autostart>true</autostart>
        </distributed-scheme>

    </caching-schemes>
</cache-config>

