<hazelcast xsi:schemaLocation="http://www.hazelcast.com/schema/config
                               http://www.hazelcast.com/schema/config/hazelcast-config-3.6.xsd"
           xmlns="http://www.hazelcast.com/schema/config"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <properties>
        <property name="hazelcast.phone.home.enabled">false</property>
    </properties>

    <group>
        <name>group</name>
    </group>

    <network>
        <port port-count="200" auto-increment="true">5701</port>
        <join>
            <multicast enabled="false"/>
            <tcp-ip enabled="true">
            </tcp-ip>
        </join>
    </network>

    <hot-restart-persistence enabled="true">
        <data-load-timeout-seconds>1700</data-load-timeout-seconds>
        <base-dir>/home/ec2-user/hot-junk</base-dir>
    </hot-restart-persistence>

    <native-memory allocator-type="POOLED" enabled="true">
        <size unit="GIGABYTES" value="2" />
        <metadata-space-percentage>20</metadata-space-percentage>
    </native-memory>

    <cache name="cacheBak0HD*">
        <in-memory-format>NATIVE</in-memory-format>
        <backup-count>0</backup-count>
        <statistics-enabled>false</statistics-enabled>
        <eviction eviction-policy="LRU" max-size-policy="FREE_NATIVE_MEMORY_PERCENTAGE" size="15"/>
        <hot-restart enabled="true">
            <fsync>false</fsync>
        </hot-restart>
        <wan-replication-ref name="wanReplication">
            <merge-policy>com.hazelcast.cache.merge.PassThroughCacheMergePolicy</merge-policy>
        </wan-replication-ref>
    </cache>

    <cache name="cacheBak1HD*">
        <in-memory-format>NATIVE</in-memory-format>
        <backup-count>1</backup-count>
        <statistics-enabled>false</statistics-enabled>
        <eviction eviction-policy="LRU" max-size-policy="FREE_NATIVE_MEMORY_PERCENTAGE" size="15"/>
        <hot-restart enabled="true">
            <fsync>false</fsync>
        </hot-restart>
        <wan-replication-ref name="wanReplication">
            <merge-policy>com.hazelcast.cache.merge.PassThroughCacheMergePolicy</merge-policy>
        </wan-replication-ref>
    </cache>

    <cache name="cacheAsync1HD*">
        <in-memory-format>NATIVE</in-memory-format>
        <backup-count>0</backup-count>
        <async-backup-count>1</async-backup-count>
        <statistics-enabled>false</statistics-enabled>
        <eviction eviction-policy="LRU" max-size-policy="FREE_NATIVE_MEMORY_PERCENTAGE" size="15"/>
        <hot-restart enabled="true">
            <fsync>false</fsync>
        </hot-restart>
        <wan-replication-ref name="wanReplication">
            <merge-policy>com.hazelcast.cache.merge.PassThroughCacheMergePolicy</merge-policy>
        </wan-replication-ref>
    </cache>


    <cache name="cacheBak0*">
        <in-memory-format>BINARY</in-memory-format>
        <backup-count>0</backup-count>
        <statistics-enabled>false</statistics-enabled>
        <eviction eviction-policy="LFU" max-size-policy="ENTRY_COUNT" size="200001"/>
        <hot-restart enabled="true">
            <fsync>false</fsync>
        </hot-restart>
        <wan-replication-ref name="wanReplication">
            <merge-policy>com.hazelcast.cache.merge.PassThroughCacheMergePolicy</merge-policy>
        </wan-replication-ref>
    </cache>

    <cache name="cacheBak1*">
        <in-memory-format>BINARY</in-memory-format>
        <backup-count>1</backup-count>
        <statistics-enabled>false</statistics-enabled>
        <eviction eviction-policy="LFU" max-size-policy="ENTRY_COUNT" size="200001"/>
        <hot-restart enabled="true">
            <fsync>false</fsync>
        </hot-restart>
        <wan-replication-ref name="wanReplication">
            <merge-policy>com.hazelcast.cache.merge.PassThroughCacheMergePolicy</merge-policy>
        </wan-replication-ref>
    </cache>

    <cache name="cacheAsync1*">
        <in-memory-format>BINARY</in-memory-format>
        <backup-count>0</backup-count>
        <async-backup-count>1</async-backup-count>
        <statistics-enabled>false</statistics-enabled>
        <eviction eviction-policy="LFU" max-size-policy="ENTRY_COUNT" size="200001"/>
        <hot-restart enabled="true">
            <fsync>false</fsync>
        </hot-restart>
        <wan-replication-ref name="wanReplication">
            <merge-policy>com.hazelcast.cache.merge.PassThroughCacheMergePolicy</merge-policy>
        </wan-replication-ref>
    </cache>




    <map name="mapBak0HD*">
        <backup-count>0</backup-count>
        <in-memory-format>NATIVE</in-memory-format>
        <eviction-policy>LRU</eviction-policy>
        <eviction-percentage>5</eviction-percentage>
        <min-eviction-check-millis>0</min-eviction-check-millis>
        <max-size policy="FREE_NATIVE_MEMORY_PERCENTAGE">15</max-size>
        <hot-restart enabled="true">
            <fsync>false</fsync>
        </hot-restart>
        <wan-replication-ref name="wanReplication">
            <merge-policy>com.hazelcast.map.merge.PassThroughMergePolicy</merge-policy>
        </wan-replication-ref>
    </map>

    <map name="mapBak1HD*">
        <backup-count>1</backup-count>
        <in-memory-format>NATIVE</in-memory-format>
        <eviction-policy>LRU</eviction-policy>
        <eviction-percentage>5</eviction-percentage>
        <min-eviction-check-millis>0</min-eviction-check-millis>
        <max-size policy="FREE_NATIVE_MEMORY_PERCENTAGE">15</max-size>
        <hot-restart enabled="true">
            <fsync>false</fsync>
        </hot-restart>
        <wan-replication-ref name="wanReplication">
            <merge-policy>com.hazelcast.map.merge.PassThroughMergePolicy</merge-policy>
        </wan-replication-ref>
    </map>

    <map name="mapAsync1HD*">
        <backup-count>0</backup-count>
        <async-backup-count>1</async-backup-count>
        <in-memory-format>NATIVE</in-memory-format>
        <eviction-policy>LRU</eviction-policy>
        <eviction-percentage>5</eviction-percentage>
        <min-eviction-check-millis>0</min-eviction-check-millis>
        <max-size policy="FREE_NATIVE_MEMORY_PERCENTAGE">15</max-size>
        <hot-restart enabled="true">
            <fsync>false</fsync>
        </hot-restart>
        <wan-replication-ref name="wanReplication">
            <merge-policy>com.hazelcast.map.merge.PassThroughMergePolicy</merge-policy>
        </wan-replication-ref>
    </map>


    <map name="mapBak0*">
        <in-memory-format>BINARY</in-memory-format>
        <backup-count>0</backup-count>
        <hot-restart enabled="true">
            <fsync>false</fsync>
        </hot-restart>
        <wan-replication-ref name="wanReplication">
            <merge-policy>com.hazelcast.map.merge.PassThroughMergePolicy</merge-policy>
        </wan-replication-ref>
    </map>

    <map name="mapBak1*">
        <in-memory-format>BINARY</in-memory-format>
        <backup-count>1</backup-count>
        <hot-restart enabled="true">
            <fsync>false</fsync>
        </hot-restart>
        <wan-replication-ref name="wanReplication">
            <merge-policy>com.hazelcast.map.merge.PassThroughMergePolicy</merge-policy>
        </wan-replication-ref>
    </map>

    <map name="mapAsync1*">
        <in-memory-format>BINARY</in-memory-format>
        <backup-count>0</backup-count>
        <async-backup-count>1</async-backup-count>
        <hot-restart enabled="true">
            <fsync>false</fsync>
        </hot-restart>
        <wan-replication-ref name="wanReplication">
            <merge-policy>com.hazelcast.map.merge.PassThroughMergePolicy</merge-policy>
        </wan-replication-ref>
    </map>

</hazelcast>
