benchId=$1

warmupSec=31
durationSec=61

for type in Client; do
 typeRegx='.*'${type}'.*'

 hzCmd set  -id ${typeRegx} -task ${benchId} -var warmupSec -val ${warmupSec}
 hzCmd set  -id ${typeRegx} -task ${benchId} -var durationSec -val ${durationSec}

 for mapName in offheap; do
  hzCmd set  -id ${typeRegx} -task ${benchId} -var name -val ${mapName}

  for threads in 8 16; do

   for size in 1001 10001; do

    hzCmd set -id ${typeRegx} -task ${benchId} -var valueSize -val ${size}
    hzCmd set -id ${typeRegx} -task ${benchId} -var title -val ${type}-${mapName}-${benchId}-${threads}threads-${size}byteValue

    hzCmd bench -id ${typeRegx} -task ${benchId} -threads ${threads}

   done
  done
 done
done