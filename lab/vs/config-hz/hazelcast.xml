<hazelcast xsi:schemaLocation="http://www.hazelcast.com/schema/config
                               http://www.hazelcast.com/schema/config/hazelcast-config-3.6.xsd"
           xmlns="http://www.hazelcast.com/schema/config"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <group>
        <name>group</name>
    </group>

    <network>
        <port port-count="200" auto-increment="true">5701</port>
        <join>
            <multicast enabled="false"/>
            <tcp-ip enabled="true">
            </tcp-ip>
        </join>
    </network>

    <native-memory allocator-type="POOLED" enabled="true">
        <size unit="GIGABYTES" value="1"/>
        <metadata-space-percentage>15</metadata-space-percentage>
    </native-memory>



    <map name="hzDefaultOffheap*">
        <in-memory-format>NATIVE</in-memory-format>
        <eviction-policy>LRU</eviction-policy>
        <max-size policy="USED_NATIVE_MEMORY_PERCENTAGE">99</max-size>
        <min-eviction-check-millis>100</min-eviction-check-millis>
        <statistics-enabled>false</statistics-enabled>
    </map>

    <map name="hzDefaultOnheap*">
        <in-memory-format>BINARY</in-memory-format>
    </map>


    <map name="hzAsyncOnheap*">
        <in-memory-format>BINARY</in-memory-format>
	<backup-count>0</backup-count>
        <async-backup-count>1</async-backup-count>
    </map>



    <cache name="hzDefaultOffheap*">
        <backup-count>1</backup-count>
        <in-memory-format>NATIVE</in-memory-format>
        <eviction size="99" max-size-policy="USED_NATIVE_MEMORY_PERCENTAGE" eviction-policy="LRU" />
        <statistics-enabled>true</statistics-enabled>
    </cache>

</hazelcast>
